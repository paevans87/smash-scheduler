{
  "name": "smash-scheduler",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/web-app",
    "dev:marketing": "npm run dev --workspace=apps/marketing",
    "build": "npm run build --workspaces",
    "build:web-app": "npm run build --workspace=apps/web-app",
    "build:marketing": "npm run build --workspace=apps/marketing",
    "test": "npm run test --workspace=apps/web-app",
    "lint": "npm run lint --workspaces"
  },
  "devDependencies": {
    "typescript": "^5"
  }
}
